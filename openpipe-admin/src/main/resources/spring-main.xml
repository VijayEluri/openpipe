<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:util="http://www.springframework.org/schema/util"
       xmlns:aop="http://www.springframework.org/schema/aop"
       xsi:schemaLocation="
       http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.0.xsd
       http://www.springframework.org/schema/util http://www.springframework.org/schema/util/spring-util-2.0.xsd
       http://www.springframework.org/schema/aop http://www.springframework.org/schema/aop/spring-aop-2.0.xsd">
       
   <bean id="stepManager" class="no.trank.openpipe.admin.biz.StepManager" init-method="init">
      <property name="configFile" value="pipelineConfigContext.xml"/>
   </bean>

   <bean id="typeManager" class="no.trank.openpipe.admin.biz.TypeManager">
      <property name="stepManager" ref="stepManager"/>
   </bean>
</beans>