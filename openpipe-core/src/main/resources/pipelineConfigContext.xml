   <bean class="no.trank.openpipe.step.ParseXML"/>
   <bean class="no.trank.openpipe.step.ParseXMLXPath"/>
   <bean class="no.trank.openpipe.step.PipelineSelector"/>
   <bean class="no.trank.openpipe.step.RegexField"/>
   <bean class="no.trank.openpipe.step.RemoveFields"/>
   <bean class="no.trank.openpipe.step.SetField"/>
   <bean class="no.trank.openpipe.step.StripHtml"/>
   <bean class="no.trank.openpipe.step.Uppercase"/>
</beans>